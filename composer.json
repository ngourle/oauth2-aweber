{
    "name": "ngourle/oauth2-aweber",
    "description": "Aweber OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Gourle",
            "email": "ngourle@gmail.com",
            "homepage": "https://github.com/ngourle"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "aweber"
    ],
    "require": {
        "php": ">=5.6",
        "league/oauth2-client": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^7.3",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Elementroot\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elementroot\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}